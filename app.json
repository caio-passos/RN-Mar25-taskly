{
  "name": "Taskly",
  "displayName": "Taskly",
  "expo": {
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Roboto-Regular.ttf"
          ],
          "android": {
            "fonts": [
              {
                "fontFamily": "Roboto",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Roboto-Bold.ttf",
                    "weight": 700,
                    "style": "bold"
                  },
                  {
                    "path": "./assets/fonts/Inter-Bold.ttf",
                    "weight": 700
                  }
                ]
              }
            ]
          },
          "ios": {
            "fonts": ["./assets/fonts/Roboto-Regular.ttf", "./assets/fonts/Roboto-Regular.ttf.ttf"]
          }
        }
      ]
    ]
  }
}
